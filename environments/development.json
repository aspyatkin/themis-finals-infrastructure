{
  "name": "development",
  "description": "Development environment",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "localdns": {
      "records": [
        {
          "type": "A",
          "name": "redis.finals.themis",
          "ipv4_address": "172.20.0.5"
        },
        {
          "type": "SRV",
          "service": "redis",
          "protocol": "tcp",
          "domain": "finals.themis",
          "target": "redis.finals.themis",
          "port": 6379
        },
        {
          "type": "A",
          "name": "postgres.finals.themis",
          "ipv4_address": "172.20.0.6"
        },
        {
          "type": "SRV",
          "service": "postgres",
          "protocol": "tcp",
          "domain": "finals.themis",
          "target": "postgres.finals.themis",
          "port": 5432
        },
        {
          "type": "A",
          "name": "master.finals.themis",
          "ipv4_address": "172.20.0.10"
        },
        {
          "type": "A",
          "name": "ruby.checker.finals.themis",
          "ipv4_address": "172.20.0.11"
        },
        {
          "type": "A",
          "name": "python2.checker.finals.themis",
          "ipv4_address": "172.20.0.12"
        },
        {
          "type": "A",
          "name": "python3.checker.finals.themis",
          "ipv4_address": "172.20.0.13"
        },
        {
          "type": "A",
          "name": "python.checker.finals.themis",
          "ipv4_address": "172.20.0.14"
        }
      ]
    },
    "themis": {
      "finals": {
        "ns": "finals.themis"
      }
    },
    "themis-finals": {
    "customize_cookbook": "volgactf-2018-final-customizer",
    "frontend_customize_module": "frontend/customize.js",
    "frontend_extra_files": {
      "frontend/volgactf-logo.svg": "src/images/volgactf-logo.svg",
      "frontend/volgactf-notify-logo.png": "src/images/volgactf-notify-logo.png"
    },
      "team_logo_files": {
      },
      "supervisor_namespace": "themis.finals",
      "flag_wrap": {
        "prefix": "ThemisFinals{",
        "suffix": "}"
      },
      "config": {
        "internal_networks": [
          "172.20.0.0/24"
        ],
        "api_req_limits": {
          "flag_info": {
            "rate": "5r/m",
            "burst": 5,
            "nodelay": true
          },
          "flag_submit": {
            "rate": "5r/m",
            "burst": 5,
            "nodelay": true
          }
        },
        "competition": {
          "title": "Themis Finals"
        },
        "settings": {
          "flag_lifetime": 360,
          "round_timespan": 120,
          "poll_timespan": 35,
          "poll_delay": 40
        },
        "deprecated_settings": {
          "attack_limit": {
            "attempts": 200,
            "time": 60
          }
        },
        "teams": {
          "team1": {
            "name": "Team #1",
            "network": "172.20.1.0/24"
          },
          "team2": {
            "name": "Team #2",
            "network": "172.20.2.0/24"
          },
          "team3": {
            "name": "Team #3",
            "network": "172.20.3.0/24"
          },
          "team4": {
            "name": "Team #4",
            "network": "172.20.4.0/24",
            "guest": true
          },
          "team5": {
            "name": "Team #5",
            "network": "172.20.5.0/24"
          },
          "team6": {
            "name": "Team #6",
            "network": "172.20.6.0/24"
          },
          "team7": {
            "name": "Team #7",
            "network": "172.20.7.0/24"
          },
          "team8": {
            "name": "Team #8",
            "network": "172.20.8.0/24",
            "guest": true
          }
        },
        "domain_files": [
          {
            "name": "init-10",
            "type": "competition_init",
            "services": {
              "ruby": {
                "name": "Ruby",
                "hostmask": "0.0.0.3",
                "checker_endpoint": "http://ruby.checker.finals.themis",
                "attack_priority": true
              }
            }
          },
          {
            "name": "update-20",
            "type": "service_init",
            "services": {
              "python": {
                "name": "Python",
                "hostmask": "0.0.0.3",
                "checker_endpoint": "http://python.checker.finals.themis",
                "attack_priority": true
              }
            }
          }
        ]
      }
    },
    "htpasswd": {
      "install_method": "ruby"
    },
    "latest-nodejs": {
      "install": "current"
    },
    "latest-redis": {
      "listen": {
        "address": "172.20.0.5",
        "port": 6379
      }
    },
    "latest-postgres": {
      "listen": {
        "address": "172.20.0.6",
        "port": 5432
      },
      "pg_hba": [
        {
          "type": "host",
          "db": "all",
          "user": "all",
          "addr": "172.20.0.0/24",
          "method": "md5"
        }
      ]
    },
    "ngx": {
      "install": "mainline",
      "with_openssl": false,
      "with_http2": false,
      "with_ipv6": false,
      "with_ct": false,
      "with_status": true,
      "with_debug": true
    },
    "ntp": {
      "servers": [
        "0.pool.ntp.org",
        "1.pool.ntp.org",
        "2.pool.ntp.org",
        "3.pool.ntp.org"
      ]
    },
    "poise-python": {
      "install_python2": true,
      "install_python3": false
    },
    "secret": {
      "prefix_fqdn": false
    }
  }
}
