{
  "environment": "production",
  "run_list": [
    "recipe[resolver::default]",
    "recipe[python::default]",
    "recipe[latest-git::default]",
    "recipe[dotfiles::default]",
    "recipe[themis-finals::default]",
    "recipe[rcc-2016-finals-digidocs-checker::default]",
    "recipe[rcc-2016-finals-mobile-profile-checker::default]",
    "recipe[rcc-2016-finals-sociality-checker::default]",
    "recipe[rcc-2016-finals-weather-checker::default]"
  ],
  "automatic": {
    "ipaddress": "production.rcc.themis-project.com"
  },
  "dotfiles": {
    "users": [
      "root",
      "rcc"
    ]
  },
  "themis-finals": {
    "user": "rcc",
    "group": "rcc",
    "backend": {
      "revision": "master",
      "debug": false,
      "queue": {
        "processes": 4
      },
      "app": {
        "processes": 4
      }
    },
    "frontend": {
      "revision": "master"
    },
    "stream": {
      "revision": "master",
      "processes": 4,
      "debug": false
    }
  },
  "rcc-2016-finals-digidocs-checker": {
    "user": "rcc",
    "group": "rcc",
    "debug": false,
    "processes": 4
  },
  "rcc-2016-finals-mobile-profile-checker": {
    "user": "rcc",
    "group": "rcc",
    "debug": false,
    "processes": 4
  },
  "rcc-2016-finals-sociality-checker": {
    "user": "rcc",
    "group": "rcc",
    "debug": false,
    "processes": 4
  },
  "rcc-2016-finals-weather-checker": {
    "user": "rcc",
    "group": "rcc",
    "debug": false,
    "processes": 4
  }
}
