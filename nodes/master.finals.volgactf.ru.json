{
  "environment": "production",
  "run_list": [
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[netdata::default]",
    "recipe[themis-finals::default]"
  ],
  "build-essential": {
    "compile_time": true
  },
  "dotfiles": {
    "users": [
      "root",
      "vctf"
    ]
  },
  "themis-finals": {
    "user": "vctf",
    "group": "vctf",
    "fqdn": "master.finals.volgactf.ru",
    "debug": true,
    "autostart": true,
    "backend": {
      "revision": "availability",
      "server": {
        "processes": 4
      },
      "queue": {
        "processes": 4
      }
    },
    "frontend": {
      "revision": "availability"
    },
    "stream": {
      "revision": "develop"
    },
    "live": {
      "fqdn": "live.finals.volgactf.ru",
      "server_username": "titan",
      "server_hostname": "aspyatkin.com",
      "remote_port": "5500"
    }
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "supervisor": {
    "inet_port": "0.0.0.0:9001"
  },
  "automatic": {
    "ipaddress": "master.finals.volgactf.ru"
  }
}
