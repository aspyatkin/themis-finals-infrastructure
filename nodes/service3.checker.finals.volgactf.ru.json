{
  "environment": "production",
  "run_list": [
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[netdata::default]",
    "recipe[themis-finals-service3-checker::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vctf"
    ]
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "themis-finals-service3-checker": {
    "user": "vctf",
    "group": "vctf",
    "revision": "develop",
    "fqdn": "service3.checker.finals.volgactf.ru",
    "debug": true,
    "autostart": true
  },
  "supervisor": {
    "inet_port": "0.0.0.0:9001"
  },
  "automatic": {
    "ipaddress": "service3.checker.finals.volgactf.ru"
  }
}
