{
  "environment": "development",
  "run_list": [
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[themis-finals-lib-attack-result-rb::default]",
    "recipe[themis-finals-lib-checker-result-rb::default]",
    "recipe[themis-finals::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vagrant"
    ]
  },
  "themis-finals": {
    "fqdn": "master.finals.themis-project.com",
    "backend": {
      "revision": "develop"
    },
    "frontend": {
      "revision": "develop"
    },
    "stream": {
      "revision": "develop"
    }
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "supervisor": {
    "inet_port": "0.0.0.0:9001"
  },
  "automatic": {
    "ipaddress": "master.finals.themis-project.com"
  }
}
