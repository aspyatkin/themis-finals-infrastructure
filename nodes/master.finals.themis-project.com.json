{
  "environment": "development",
  "run_list": [
    "recipe[locale::default]",
    "recipe[ntp::default]",
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[netdata::default]",
    "recipe[latest-redis::default]",
    "recipe[latest-nodejs::default]",
    "recipe[modern_nginx::default]",
    "recipe[supervisor::default]",
    "recipe[yarn::default]",
    "recipe[themis-finals-lib-attack-result-rb::default]",
    "recipe[themis-finals-lib-checker-result-rb::default]",
    "recipe[themis-finals::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vagrant"
    ]
  },
  "themis-finals": {
    "fqdn": "master.finals.themis-project.com",
    "debug": true,
    "autostart": true,
    "backend": {
      "revision": "master",
      "server": {
        "processes": 4
      },
      "queue": {
        "processes": 4
      }
    },
    "frontend": {
      "revision": "master"
    },
    "stream": {
      "revision": "master"
    },
    "live": {
      "enable": false,
      "fqdn": "live.finals.themis-project.com",
      "server_username": "titan",
      "server_hostname": "aspyatkin.com",
      "remote_port": "5000"
    }
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "latest-nodejs": {
    "install": "lts",
    "binary": true
  },
  "supervisor": {
    "inet_port": "0.0.0.0:9001"
  },
  "automatic": {
    "ipaddress": "master.finals.themis-project.com"
  }
}
