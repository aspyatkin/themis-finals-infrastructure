{
  "environment": "development",
  "run_list": [
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[themis-finals::prerequisite_ssh_keys]",
    "recipe[themis-finals-service2-checker::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vagrant"
    ]
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "themis-finals-service2-checker": {
    "revision": "develop",
    "fqdn": "service2.checker.finals.themis-project.com",
    "source_packages": false
  },
  "themis-finals-checker-app-py-lib": {
    "revision": "develop"
  },
  "themis-finals-checker-result-py-lib": {
    "revision": "develop"
  },
  "themis-finals-api-auth-py-lib": {
    "revision": "develop"
  },
  "automatic": {
    "ipaddress": "service2.checker.finals.themis-project.com"
  }
}
