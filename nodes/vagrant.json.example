{
  "redisio": {
    "version": "3.0.2"
  },
  "postgresql": {
    "version": "9.4",
    "password": {
      "postgres": "100cf32c205cbe9f41af3c738d12a4ee"
    },
    "server": {
      "packages": ["postgresql-9.4", "postgresql-server-dev-9.4", "libpq-dev"]
    },
    "enable_pgdg_apt": true,
    "dir": "/etc/postgresql/9.4/main",
    "config": {
      "data_directory": "/var/lib/postgresql/9.4/main",
      "hba_file": "/etc/postgresql/9.4/main/pg_hba.conf",
      "ident_file": "/etc/postgresql/9.4/main/pg_ident.conf",
      "external_pid_file": "/var/run/postgresql/9.4-main.pid",
      "listen_addresses": "127.0.0.1",
      "port": 5432
    }
  },
  "beanstalkd": {
    "start_during_boot": true,
    "opts": {
      "l": "127.0.0.1",
      "p": 11300
    }
  },
  "local-rbenv": {
    "group_users": ["vagrant", "root"]
  },
  "themis": {
    "user": "vagrant",
    "group": "vagrant",
    "log_level": "DEBUG",
    "stdout_sync": "true",
    "queue": {
      "processes": 2
    },
    "backend": {
      "processes": 2
    },
    "stream": {
      "processes": 2
    }
  },
  "sample-checker-rb": {
    "service": "service_1",
    "user": "vagrant",
    "group": "vagrant",
    "log_level": "DEBUG",
    "stdout_sync": "true",
    "processes": 2
  },
  "sample-checker-py":{
    "service": "service_2",
    "user": "vagrant",
    "group": "vagrant",
    "log_level": "DEBUG",
    "processes": 2
  },
  "python": {
  },
  "oh-my-zsh": {
    "entries": [
      {
        "user": "vagrant",
        "group": "vagrant"
      },
      {
        "user": "root",
        "group": "root"
      }
    ],
    "plugins": [
      "git",
      "rbenv",
      "virtualenv"
    ]
  },
  "ntp": {
    "servers": [
      "0.pool.ntp.org",
      "1.pool.ntp.org",
      "2.pool.ntp.org",
      "3.pool.ntp.org"
    ]
  },
  "run_list": [
    "recipe[oh-my-zsh]",
    "recipe[redisio]",
    "recipe[redisio::enable]",
    "recipe[postgresql::server]",
    "recipe[beanstalkd]",
    "recipe[database::postgresql]",
    "recipe[python::default]",
    "recipe[python::pip]",
    "recipe[python::virtualenv]",
    "recipe[ntp::default]",
    "recipe[themis]",
    "recipe[sample-checker-rb]",
    "recipe[sample-checker-py]"
  ]
}