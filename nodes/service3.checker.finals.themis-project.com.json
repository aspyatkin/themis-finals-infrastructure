{
  "environment": "development",
  "run_list": [
    "recipe[ntp::default]",
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[netdata::default]",
    "recipe[latest-redis::default]",
    "recipe[modern_nginx::default]",
    "recipe[supervisor::default]",
    "recipe[themis-finals-service3-checker::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vagrant"
    ]
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "themis-finals-service3-checker": {
    "revision": "master",
    "fqdn": "service3.checker.finals.themis-project.com",
    "debug": true,
    "autostart": true,
    "queue": {
      "processes": 4
    }
  },
  "supervisor": {
    "inet_port": "0.0.0.0:9001"
  },
  "automatic": {
    "ipaddress": "service3.checker.finals.themis-project.com"
  }
}
