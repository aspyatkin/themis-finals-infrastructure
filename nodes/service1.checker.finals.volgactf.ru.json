{
  "environment": "production",
  "run_list": [
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[themis-finals::prerequisite_ssh_keys]",
    "recipe[themis-finals-service1-checker::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vctf"
    ]
  },
  "rbenv": {
    "group_users": [
      "root",
      "vctf"
    ]
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "themis-finals-service1-checker": {
    "user": "vctf",
    "group": "vctf",
    "fqdn": "service1.checker.finals.volgactf.ru"
  },
  "automatic": {
    "ipaddress": "service1.checker.finals.volgactf.ru"
  }
}
